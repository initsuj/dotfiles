Include config.d/*

# DevPod Start mandark-go.devpod
Host mandark-go.devpod
  ForwardAgent yes
  LogLevel error
  StrictHostKeyChecking no
  UserKnownHostsFile /dev/null
  HostKeyAlgorithms rsa-sha2-256,rsa-sha2-512,ssh-rsa
  ProxyCommand "/usr/local/bin/devpod" ssh --stdio --context default --user vscode mandark-go --workdir "/workspace"
  User vscode
# DevPod End mandark-go.devpod
